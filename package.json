{
  "name": "unode",
  "version": "1.0.0",
  "description": "A high-performance Node.js framework built on uWebSockets.js for fast and efficient web servers.",
  "keywords": [
    "uWebSockets.js",
    "Node.js",
    "Framework",
    "Web Server"
  ],
  "author": "Joel",
  "license": "MIT",
  "main": "dis/src/index.js",
  "type": "module",
  "scripts": {
    "build": "swc src -d dist",
    "start": "node dist/src/index.js",
    "dev": "nodemon -L -e ts --exec \"npm run build && npm start\""
  },
  "dependencies": {
    "better-sqlite3": "^11.1.1",
    "mrmime": "^2.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.44.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.6.5",
    "@types/better-sqlite3": "^7.6.10",
    "@types/ip": "^1.1.3",
    "@types/node": "^20.14.9",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "nodemonConfig": {
    "ignore": [
      "dist"
    ]
  }
}
