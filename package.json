{
  "name": "@oki.gg/unode",
  "version": "1.3.0",
  "description": "A high-performance Node.js framework built on uWebSockets.js for fast and efficient web servers.",
  "keywords": [
    "node",
    "uWebSockets",
    "framework",
    "fast",
    "performance"
  ],
  "author": "Joel",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && node rename-files.js && node fix-imports.js",
    "start": "node dist/esm/index.mjs"
  },
  "dependencies": {
    "mrmime": "^2.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.44.0"
  },
  "devDependencies": {
    "@types/ip": "^1.1.3",
    "@types/node": "^20.14.9",
    "typescript": "^5.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Joel-Adving/uNode.git"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  }
}
